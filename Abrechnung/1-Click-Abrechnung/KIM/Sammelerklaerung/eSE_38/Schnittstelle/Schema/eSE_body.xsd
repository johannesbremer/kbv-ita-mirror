				<xs:annotation>
					<xs:documentation>Erklärung zur Abrechnung von Leistungen des Allgemeinlabors</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="avwg_erklaerung" type="avwg_erklaerung_typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Erklärung zum Einsatz AVWG-zertifizierter Arzneimittelverordnungssoftware (AVS)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>==============================================================================</xs:documentation>
	</xs:annotation>
	<xs:complexType name="abrechnung_erklaerung_typ">
		<xs:annotation>
			<xs:documentation>Typ zur Bestätigung zur Quartalsabrechnung allgemein</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="titel"/>
			<xs:element ref="text_liste" minOccurs="0"/>
			<xs:element ref="bool_einfach">
				<xs:annotation>
					<xs:documentation>Bestätigung eines generischen Sachverhalts ohne Zusatzinformationen</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>--------------------------------------------------------------------------------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:complexType name="ueberweisungsscheine_typ">
		<xs:annotation>
			<xs:documentation>Typ zur Bestätigung der Abrechnung korrekter Überweisungsscheine unter Einhaltung des jeweiligen Zielauftrages</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="titel"/>
			<xs:element ref="text_liste" minOccurs="0"/>
			<xs:element ref="bool_einfach">
				<xs:annotation>
					<xs:documentation>Bestätigung eines generischen Sachverhalts ohne Zusatzinformationen</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>--------------------------------------------------------------------------------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:complexType name="abrechnung_edv_typ">
		<xs:annotation>
			<xs:documentation>Typ für die Abbildung der Erklärung zum EDV-Einsatz in der Abrechnung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="titel"/>
			<xs:element ref="text_liste" minOccurs="0"/>
			<xs:element ref="bool_software">
				<xs:annotation>
					<xs:documentation>Bestätigung des Einsatzes einer Software und Beschreibung der Software mittels bekannter sciphox-ssu</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>--------------------------------------------------------------------------------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:complexType name="abrechnung_allgemeinlabor_typ">
		<xs:annotation>
			<xs:documentation>Struktur für die Bestätigung bei Abrechnung von Leistungen des Allgemeinlabors</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="titel"/>
			<xs:element ref="text_liste" minOccurs="0"/>
			<xs:element ref="bool_labor">
				<xs:annotation>
					<xs:documentation>Bestätigung der Mitgliedschaft an einer Laborgemeinschaft und/oder Qualifikation zur Erbringung von Leistungen des Allgemeinlabors</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="labor_provider_typ">
		<xs:annotation>
			<xs:documentation>Providertyp - Labor</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="organization" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>--------------------------------------------------------------------------------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:complexType name="avwg_erklaerung_typ">
		<xs:annotation>
			<xs:documentation>Typ für die Abbildung der AVWG-Erklärung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="titel"/>
			<xs:element ref="text_liste" minOccurs="0"/>
			<xs:element ref="bool_software">
				<xs:annotation>
					<xs:documentation>Bestätigung des Einsatzes einer Software und Beschreibung der Software mittels bekannter sciphox-ssu</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>--------------------------------------------------------------------------------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:complexType name="ankreuzfeld_liste_typ">
		<xs:annotation>
			<xs:documentation>Generische Struktur zur Abbildung einfacher Ankreuzfelder bzw. Bestätigungen</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ankreuzfeld" type="ankreuzfeld_typ" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ankreuzfeld_typ">
		<xs:annotation>
			<xs:documentation>Ankreuzfeld-Typ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="titel"/>
			<xs:element ref="text_liste" minOccurs="0"/>
			<xs:element ref="bool_komplex"/>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>==============================================================================</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>Innerhalb des Body referenzierte Elemente/Typen:</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>==============================================================================</xs:documentation>
	</xs:annotation>
	<xs:element name="bool_komplex" type="bool_komplex_typ">
		<xs:annotation>
			<xs:documentation>Bestätigung eines generischen Sachverhalts mit Zusatzinformationen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bool_komplex_typ">
		<xs:choice>
			<xs:element name="false"/>
			<xs:element name="true">
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="komplex" type="zeitraum_mit_arztbezug_typ" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:element name="bool_einfach" type="bool_einfach_typ">
		<xs:annotation>
			<xs:documentation>Bestätigung eines generischen Sachverhalts ohne Zusatzinformationen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bool_einfach_typ">
		<xs:choice>
			<xs:element name="false"/>
			<xs:element name="true">
				<xs:complexType/>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:element name="bool_software" type="bool_software_typ">
		<xs:annotation>
			<xs:documentation>Bestätigung  des Einsatzes einer Software und Angabe derselben mittels bekannter sciphox-ssu</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bool_software_typ">
		<xs:choice>
			<xs:element name="false"/>
			<xs:element name="true">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="sciphox:sciphox-ssu" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:element name="bool_labor" type="bool_labor_typ">
		<xs:annotation>
			<xs:documentation>Bestätigung zur Abrechnung von leistungen des Allgemeinlabors mit Zusatzinformationen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bool_labor_typ">
		<xs:choice>
			<xs:element name="false"/>
			<xs:element name="true">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="provider" type="labor_provider_typ" minOccurs="0"/>
						<xs:element name="qualifikation_labor" minOccurs="0">
							<xs:complexType>
								<xs:attribute name="V" type="xs:boolean" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:element name="text_liste" type="text_liste_typ">
		<xs:annotation>
			<xs:documentation>Struktur für integrierte Textbausteine</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="text_liste_typ">
		<xs:sequence>
			<xs:element ref="text" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Textbaustein</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="text" type="text_typ">
		<xs:annotation>
			<xs:documentation>Textbaustein ohne Formatierungsinformationen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="text_typ">
		<xs:attribute name="V" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="titel" type="text_typ">
		<xs:annotation>
			<xs:documentation>Titel, Überschrift</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="zeitraum_tmr" type="v_zeitraum_typ">
		<xs:annotation>
			<xs:documentation>Angabe eines Datums oder Zeitraumes (datum..datum oder datum zeit..datum zeit)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="organization.nm">
		<xs:annotation>
			<xs:documentation>Name der Einrichtung, Praxisbezeichnung</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="v_string_typ"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="grund">
		<xs:annotation>
			<xs:documentation>Grund bei Vertretung oder Abwesenheit</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="V" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Krankheit"/>
						<xs:enumeration value="Urlaub"/>
						<xs:enumeration value="Fortbildung"/>
						<xs:enumeration value="Schwangerschaft"/>
						<xs:enumeration value="Entbindung"/>
						<xs:enumeration value="Elternzeit"/>
						<xs:enumeration value="Wehrübung"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="xslt_hash_typ">
		<xs:annotation>
			<xs:documentation>MD5-Hashwert-Typ</xs:documentation>
		</xs:annotation>
		<xs:attribute name="V" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="zeitraum_mit_arztbezug_typ">
		<xs:annotation>
			<xs:documentation>Struktur, die einen Zeitraum definiert, mit Arztbezug (Provider)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="grund" minOccurs="0"/>
			<xs:element ref="zeitraum_tmr" minOccurs="0"/>
			<xs:element ref="provider" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
