    <element id="DocumentReference">
      <path value="DocumentReference" />
      <constraint>
        <key value="NotAllOrNothingHASOne" />
        <severity value="error" />
        <human value="Min. eine aber auch nur eine Referenz aus Betriebsstaette, Patient und Begegnung ist erlaubt." />
        <expression value="subject.reference.exists() xor context.encounter.reference.exists() xor context.related.reference.exists()" />
        <source value="https://fhir.kbv.de/StructureDefinition/KBV_PR_AW_Anlage" />
      </constraint>
      <constraint>
        <key value="SpezifikationsVersionKBVMuster" />
        <severity value="error" />
        <human value="Ist ein KBV-Muster gewählt worden muss die Spezifikationsversion im Format 1-4QJJJJ angegeben werden" />
        <expression value="type.coding.code.startsWith('KV-Muster') implies masterIdentifier.value.matches('[1-4][qQ]\\d{4}')" />
        <source value="https://fhir.kbv.de/StructureDefinition/KBV_PR_AW_Anlage" />
      </constraint>
      <constraint>
        <key value="SpezifikationsVersionDPE" />
        <severity value="error" />
        <human value="Für DPE ist die Spezifikationsversion im Format x.x.x anzugegeben" />
        <expression value="type.coding.code.startsWith('DPE') implies masterIdentifier.value.matches('\\d{1}\\.\\d{1}\\.\\d{1}')" />
        <source value="https://fhir.kbv.de/StructureDefinition/KBV_PR_AW_Anlage" />
      </constraint>
      <constraint>
        <key value="SpezifikationsVersionNFD" />
        <severity value="error" />
        <human value="Für NFD ist die Spezifikationsversion im Format x.x.x anzugegeben" />
        <expression value="type.coding.code.startsWith('NFD') implies masterIdentifier.value.matches('\\d{1}\\.\\d{1}\\.\\d{1}')" />
        <source value="https://fhir.kbv.de/StructureDefinition/KBV_PR_AW_Anlage" />
      </constraint>
      <constraint>
        <key value="EnteredInError" />
        <severity value="error" />
        <human value="Der Status &quot;Entered in Error&quot; ist nicht erlaubt" />
        <expression value="status!='entered-in-error'" />
        <source value="https://fhir.kbv.de/StructureDefinition/KBV_PR_AW_Anlage" />
      </constraint>	  
