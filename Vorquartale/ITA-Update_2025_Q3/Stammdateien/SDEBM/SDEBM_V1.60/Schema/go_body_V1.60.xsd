<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns:ehd="urn:ehd/001" xmlns:go="urn:ehd/go/001" xmlns="urn:ehd/go/001" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:ehd/go/001" elementFormDefault="qualified">
	<!-- ===================================================	SCHEMASPEZIFIKATION DER SA 850-853 ================ VERSION 1.60 =====================================================================================-->
	<xs:include schemaLocation="go_datentypen_V1.60.xsd"/>
	<xs:annotation>
		<xs:documentation>DEFINITIONEN</xs:documentation>
	</xs:annotation>
	<xs:element name="gnr_liste" type="gnr_liste_typ">
		<xs:annotation>
			<xs:documentation>Klammerelement füe die GNR-Schlüsselelemente</xs:documentation>
		</xs:annotation>
		<xs:unique name="u_gnr_V">
			<xs:selector xpath="go:gnr"/>
			<xs:field xpath="@V"/>
		</xs:unique>
	</xs:element>
	<xs:complexType name="gnr_liste_typ">
		<xs:sequence>
			<xs:element ref="go:gnr" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="gnr" type="go:gnr_typ">
		<xs:annotation>
			<xs:documentation>Schlüsselelement im Body-Bereich</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="gnr_typ">
		<xs:sequence>
			<xs:element ref="go:allgemein"/>
			<xs:element ref="go:bedingung" minOccurs="0"/>
			<xs:element ref="go:regel" minOccurs="0"/>
			<xs:element ref="go:kv" minOccurs="0"/>
			<xs:element ref="go:vdx" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="V" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="\d{5}|\d{5}\p{Lu}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="USE" type="xs:string" use="required"/>
		<xs:attribute name="USE-DOMAIN" use="required" fixed="1.2.276.0.76.5.233">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="1.2.276.0.76.5.233"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="VT" type="vt_typ" use="required"/>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>SEKTIONEN</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="allgemein" type="go:allgemein_typ">
		<xs:annotation>
			<xs:documentation>Sektion mit allgemeingültigen Informationen zur GOP</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="allgemein_typ">
		<xs:sequence>
			<xs:element ref="gueltigkeit"/>
			<xs:element ref="legende"/>
			<xs:element ref="anmerkungen_liste" minOccurs="0"/>
			<xs:element ref="leistungsinhalt_obligat" minOccurs="0"/>
			<xs:element ref="leistungsinhalt_fakultativ" minOccurs="0"/>
			<xs:element ref="bewertung_liste" minOccurs="0"/>
			<xs:element ref="zeitbedarf_liste" minOccurs="0"/>
			<xs:element ref="pruefzeit" minOccurs="0"/>
			<xs:element ref="leistungsgruppe" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="bedingung" type="go:bedingung_typ">
		<xs:annotation>
			<xs:documentation>Sektion mit Informationen zu Bedingungen, die als Voraussetzung zur Abrechnung erfüllt sein müssen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bedingung_typ">
		<xs:sequence>
			<xs:element ref="fachgruppe_liste" minOccurs="0"/>
			<xs:element name="berichtspflicht" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dokumentiert, ob ggf. ein Arztbrief erstellt werden muss.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="genehmigungspflicht" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dokumentiert, ob ggf. erst eine besondere Genehmigungspflicht durch die KV zur Abrechnung der Leistung berechtigt.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element ref="go:aop_115b" minOccurs="0"/>
			<xs:element ref="gnr_zusatzangaben" minOccurs="0"/>
			<xs:element ref="begruendungen_liste" minOccurs="0"/>
			<xs:element name="administrative_gender_cd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Stellt einen Geschlechtsbezug der Leistung her.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
					<xs:attribute name="S" use="required" fixed="1.2.276.0.76.5.232">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="1.2.276.0.76.5.232"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="altersbedingung_liste" minOccurs="0"/>
			<xs:element ref="anzahlbedingung_liste" minOccurs="0"/>
			<xs:element ref="scheinarten_liste" minOccurs="0"/>
			<xs:element name="abr_best" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Abrechnungsbestimmung als Fliestext</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="regel" type="go:regel_typ">
		<xs:annotation>
			<xs:documentation>Sektion mit Informationen zur Beregelung</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="regel_typ">
		<xs:sequence>
			<xs:element ref="ausschluss_liste" minOccurs="0"/>
			<xs:element ref="go:grundleistungen_liste" minOccurs="0"/>
			<xs:element ref="go:sub_gop_liste" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Verweis auf Sub-GOP, die als Spezialisierung von Pauschalen oder in Form von Strukturzuschlägen zugesetzt werden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="go:pfg_ausschluss" minOccurs="0"/>
			<xs:element ref="ersetzungs_liste" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="kv" type="go:kv_typ">
		<xs:annotation>
			<xs:documentation>Sektion mit Informationen über regionale Besonderheiten /Abweichungen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="kv_typ">
		<xs:sequence>
			<xs:element ref="kennzeichen"/>
			<xs:element ref="kv_gruppierungsfeld_liste" minOccurs="0"/>
			<xs:element ref="kv_bewertung_liste" minOccurs="0"/>
			<xs:element ref="ktgruppen_liste" minOccurs="0"/>
			<xs:element ref="kt_liste" minOccurs="0"/>
			<xs:element ref="ktab_liste" minOccurs="0"/>
			<xs:element ref="abrechnungsstelle_liste" minOccurs="0"/>
			<xs:element ref="as_ktgruppen_liste" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="V" type="xs:string" use="required"/>
		<xs:attribute name="S" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="1.2.276.0.76.5.233"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="vdx" type="go:vdx_typ">
		<xs:annotation>
			<xs:documentation>Sektion mit relevante Daten zum Vertragsdatensatz (VDX)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="vdx_typ">
		<xs:sequence>
			<xs:element name="vertragsart" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
					<xs:attribute name="S" use="required" fixed="1.2.276.0.76.5.257">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="1.2.276.0.76.5.257"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="gkv_kontenart_liste" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gkv_kontenart" maxOccurs="unbounded">
							<!-- *************************************************************Element gkv_konto************* -->
							<xs:complexType>
								<xs:attribute name="V" type="xs:string" use="required"/>
								<xs:attribute name="S" use="required" fixed="1.2.276.0.76.5.258">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="1.2.276.0.76.5.258"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
				<xs:unique name="u_kontenart_V">
					<xs:selector xpath="go:gkv_kontenart"/>
					<xs:field xpath="@V"/>
				</xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>ELEMENTE UND TYPEN</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="gueltigkeit" type="go:gueltigkeit_typ">
		<xs:annotation>
			<xs:documentation>Abbildung der fachlichen und technischen Gültigkeit einer GOP</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="gueltigkeit_typ">
		<xs:sequence>
			<xs:element name="service_tmr">
				<xs:annotation>
					<xs:documentation>Die fachliche Gültigkeit gibt an, seit wann eine Ziffer mit einem bestimmten Leistungsinhalt existiert und abrechenbar ist oder war.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="zeitraum_typ" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="effective_tmr" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dieses Element definiert den technischen Gültigkeitsbeginn oder -zeitraum einer Leistung in der aktuellen Ausprägung.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="zeitraum_typ" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="legende" type="go:legende_typ">
		<xs:annotation>
			<xs:documentation>Klammerelement für hierarchische und systematische Zuordnung einer GOP</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="legende_typ">
		<xs:sequence>
			<xs:element name="kap_bez" type="kapiteldetails_typ">
				<xs:annotation>
					<xs:documentation>Kapitelpfad</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kurztext">
				<xs:annotation>
					<xs:documentation>Überschrift</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="langtext" type="go:ebm_text_absatz" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Beschreibungstext der GNR, bei Katalogleistungen Beschreibungstext des Kataloges</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="langtext_continued" type="go:ebm_text_absatz" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Beschreibungstext bei abgeleiteten Leistungen, Text der Katalogleistung bei Katalogleistungen, ansonsten nicht gefüllt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="quittungstext" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Verständliche Kurzfassung für die Patientenquittung.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:complexType name="kapiteldetails_typ">
		<xs:annotation>
			<xs:documentation>Klammerelement für die Abbildung der Kapitelhierarchie</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bereich">
				<xs:annotation>
					<xs:documentation>Systematische Einordnung im Rahmen des EBM.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
					<xs:attribute name="DN" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="kapitel">
				<xs:annotation>
					<xs:documentation>Beinhaltet die Kapitelangabe.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
					<xs:attribute name="DN" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="abschnitt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Zweite Hierarchieebene unterhalb des Kapitels. Wird im Rahmen des EBM als Abschnitt bezeichnet.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
					<xs:attribute name="DN" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="uabschnitt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dritte Hierarchieebene unterhalb des Kapitels. Wird im Rahmen des EBM als Unterabschnitt bezeichnet.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
					<xs:attribute name="DN" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="block" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vierte Hierarchieebene unterhalb des Kapitels. Wird im Rahmen des EBM als Block bezeichnet.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
					<xs:attribute name="DN" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="V" type="xs:string" use="required"/>
		<xs:attribute name="DN" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="anmerkungen_liste" type="go:anmerkungen_liste_typ">
		<xs:annotation>
			<xs:documentation>Klammerlement für sonstige Informationen im Fließtext</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="anmerkungen_liste_typ">
		<xs:sequence>
			<xs:element name="anmerkung" type="go:ebm_text_absatz" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Ausformulierte Regeln oder Abrechnungshinweise des EBM2000plus. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="leistungsinhalt_obligat" type="go:ebm_text_absatz">
		<xs:annotation>
			<xs:documentation>obligater Leistungsinhalt des GNR-Textes</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="leistungsinhalt_fakultativ" type="go:ebm_text_absatz">
		<xs:annotation>
			<xs:documentation>fakultativer Leistungsinhalt des GNR-Textes</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="bewertung_liste" type="go:bewertung_liste_typ">
		<xs:annotation>
			<xs:documentation>Klammerelement für eine Liste von Bewertungen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bewertung_liste_typ">
		<xs:sequence>
			<xs:element ref="bewertung" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="bewertung" type="bewertung_typ">
		<xs:annotation>
			<xs:documentation>Abbildung von EBM-Bewertungen mit differenzierten Kriterien</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bewertung_typ">
		<xs:sequence>
			<xs:element name="leistungserbringerart" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Einschränkende Zuordnung des ambulanten bzw. stationären Sektors.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" type="xs:string" use="required"/>
					<xs:attribute name="S" use="required" fixed="1.2.276.0.76.5.234">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="1.2.276.0.76.5.234"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="leistung_typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Leistungsart der zugrundeliegenden Bewertung AL, TL, GL</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="V" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="AL"/>
								<xs:enumeration value="TL"/>
								<xs:enumeration value="GL"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="versorgungsgebiet" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="V" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="-{0,1}\d*|-{0,1}\d*\.\d{1,2}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="U" type="xs:string" use="required"/>
		<xs:attribute name="U-DOMAIN" use="required" fixed="1.2.276.0.76.5.236">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="1.2.276.0.76.5.236"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>------------------------------------------</xs:documentation>
	</xs:annotation>
	<xs:element name="zeitbedarf_liste" type="go:zeitbedarf_liste_typ">
		<xs:annotation>
			<xs:documentation>Abbildung der Kalkulationszeit (betriebswirtschaftliche Grundlage)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="zeitbedarf_liste_typ">
		<xs:sequence>
			<xs:element name="zeit" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Zeitangabe mit Einheit.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="leistung_typ" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Leistungsart der zugrundeliegenden Kalkulation (Ärztliche Leistung, Technische Leistung, Gesamtleistung). Wird derzeit ausschließlich für die Gesamtleistung angegeben und kann daher auch entfallen.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="V" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="AL"/>
											<xs:enumeration value="TL"/>
											<xs:enumeration value="GL"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
