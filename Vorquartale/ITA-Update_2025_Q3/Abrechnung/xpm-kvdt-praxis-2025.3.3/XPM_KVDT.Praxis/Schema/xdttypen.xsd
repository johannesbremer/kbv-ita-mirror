		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{1,5}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 6 -->
	<xsd:simpleType name="NUMMER6">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{6}"/>
		</xsd:restriction>
	</xsd:simpleType>
