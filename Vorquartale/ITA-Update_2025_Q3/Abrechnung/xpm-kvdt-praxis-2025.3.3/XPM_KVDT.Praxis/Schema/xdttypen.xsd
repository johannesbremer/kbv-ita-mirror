	<xsd:simpleType name="NUMMER5">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{5}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld mit max.Länge 5 -->
