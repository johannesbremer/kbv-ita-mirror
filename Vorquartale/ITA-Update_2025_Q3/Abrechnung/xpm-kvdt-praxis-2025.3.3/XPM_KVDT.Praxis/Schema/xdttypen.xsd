		</xsd:restriction>
	</xsd:simpleType>

	<!-- Nummerisches Feld mit Inhalt 1 -->
	<xsd:simpleType name="NUMMERWERT1">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld von 1 bis 2 -->
	<xsd:simpleType name="NUMMER1_BIS_2">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld von 1 bis 3 -->
	<xsd:simpleType name="NUMMER1_BIS_3">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 1 bis 2 -->
	<xsd:simpleType name="NUMMER_LEN_1_BIS_2">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{1,2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 1 bis 4 -->
	<xsd:simpleType name="NUMMER_LEN_1_BIS_4">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{1,4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 2 -->
	<xsd:simpleType name="NUMMER2">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 2, > 0 -->
	<xsd:simpleType name="NUMMER2_POSITIV">
		<xsd:restriction base="xsd:positiveInteger">
			<xsd:maxInclusive value="99"/>
			<xsd:pattern value="\d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 2, > 1 -->
	<xsd:simpleType name="NUMMER2_POSITIV_AB_2">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="02"/>
			<xsd:maxInclusive value="99"/>
			<xsd:pattern value="\d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 3 -->
	<xsd:simpleType name="NUMMER3">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 3 -->
	<xsd:simpleType name="NUMMER_1_bis_3">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{1,3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 3, > 0 -->
	<xsd:simpleType name="NUMMER3_POSITIV">
		<xsd:restriction base="xsd:positiveInteger">
			<xsd:maxInclusive value="999"/>
			<xsd:pattern value="\d{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NUMMER_LEN_BIS_3">
		<xsd:restriction base="xsd:string">
           <xsd:pattern value="[0-9]{1,3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 4 -->
	<xsd:simpleType name="NUMMER4">
		<xsd:restriction base="xsd:string">
