	<!-- Nummerisches Feld von 0 bis 3 ohne 1-->
	<xsd:simpleType name="NUMMER0_BIS_3_OHNE_1">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld von 1 bis 7 -->
