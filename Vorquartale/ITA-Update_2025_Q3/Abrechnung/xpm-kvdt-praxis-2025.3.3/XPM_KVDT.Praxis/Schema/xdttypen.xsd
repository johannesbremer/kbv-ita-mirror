	<!-- Leistungskennzeichen -->
	<xsd:simpleType name="LEISTUNG_KZ">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\d|[A-Z]|#|$|&lt;|&gt;|\*|k){1,3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Datumsformat MMJJ -->
	<xsd:simpleType name="MONAT_JAHR">
		<xsd:restriction base="xsd:unsignedShort">
			<xsd:pattern value="(0[1-9]|1[0-2])[0-9]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld von 0 bis 1 -->
	<xsd:simpleType name="NUMMER0_BIS_1">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld von 0 bis 2 -->
	<xsd:simpleType name="NUMMER0_BIS_2">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld von 0 bis 3 -->
	<xsd:simpleType name="NUMMER0_BIS_3">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld von 0 bis 4 -->
	<xsd:simpleType name="NUMMER0_BIS_4">
