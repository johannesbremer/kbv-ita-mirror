			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
			<xsd:enumeration value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Nummerisches Feld der Länge 1 -->
	<xsd:simpleType name="NUMMER1">
		<xsd:restriction base="xsd:unsignedByte">
			<xsd:totalDigits value="1"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="NUMMER0">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
		</xsd:restriction>
	</xsd:simpleType>

	<!-- Nummerisches Feld der Länge 1 -->
	<xsd:simpleType name="NUMMER_LEN_1">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d"/>
		</xsd:restriction>
	</xsd:simpleType>

