<?xml version="1.0" encoding="ISO-8859-1"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<!--
		Basistypen
	-->
	<!-- Abrechnungsart -->
	<xsd:simpleType name="ABRECHNUNGSART">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Abrechnungsgebiet -->
	<xsd:simpleType name="ABRGEB">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="00"/>
			<xsd:enumeration value="01"/>
			<xsd:enumeration value="02"/>
			<xsd:enumeration value="03"/>
			<xsd:enumeration value="04"/>
			<xsd:enumeration value="05"/>
			<xsd:enumeration value="06"/>
			<xsd:enumeration value="07"/>
			<xsd:enumeration value="08"/>
			<xsd:enumeration value="09"/>
			<xsd:enumeration value="10"/>
			<xsd:enumeration value="14"/>
			<xsd:enumeration value="15"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Altersgrenze -->
	<xsd:simpleType name="ALTERSGRENZE">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(t[0-3][0-9][0-9])|(j[0-1][0-9][0-9])"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gültige Datumsangabe in der Form TTMMJJJJ -->
	<xsd:simpleType name="DATUM">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="(((((0[1-9]|[12][0-9]|3[01])(0[13578]|10|12))|((0[1-9]|[12][0-9]|30)(0[469]|11))|((0[1-9]|1[0-9]|2[0-8])(02)))((1[89](\d{2}))|([2-9](\d{3}))))|((29)(02)((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))))"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gültige Datumsangaben in der Form JJJJMMTT -->
	<xsd:simpleType name="DATUM_NEU">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="(((((1[89](\d{2}))|([2-9](\d{3})))(((0[13578]|10|12)(0[1-9]|[12][0-9]|3[01]))|((0[469]|11)(0[1-9]|[12][0-9]|30))|((02)(0[1-9]|1[0-9]|2[0-8])))))|(((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))(02)(29)))"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Datumsangaben in der Form JJJJMMTT ohne Jahres-Plausibilitätsprüfung -->
	<xsd:simpleType name="DATUM_ISO">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="\d{4}(0[0-9]|1[012])(0[0-9]|[12][0-9]|3[01])"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Diagnosensicherheit: Verdacht auf, Zustand nach,ausgeschlossen und gesicherte Diagnose -->
	<xsd:simpleType name="DIAGNOSE">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="V"/>
			<xsd:enumeration value="Z"/>
			<xsd:enumeration value="A"/>
			<xsd:enumeration value="G"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- DMP-Kennzeichnung (DMP) -->
	<xsd:simpleType name="DMP">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="4"/>
			<xsd:enumeration value="6"/>
			<xsd:enumeration value="7"/>
			<xsd:enumeration value="8"/>
			<xsd:enumeration value="9"/>
			<xsd:enumeration value="M"/>
			<xsd:enumeration value="X"/>
			<xsd:enumeration value="A"/>
			<xsd:enumeration value="C"/>
			<xsd:enumeration value="K"/>
			<xsd:enumeration value="L"/>
			<xsd:enumeration value="E"/>
			<xsd:enumeration value="N"/>
			<xsd:enumeration value="D"/>
			<xsd:enumeration value="F"/>
			<xsd:enumeration value="S"/>
			<xsd:enumeration value="P"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gültige Datumsangaben in der Form TTMMJJJJTTMMJJJJ -->
	<xsd:simpleType name="DOPPEL_DATUM">
		<xsd:restriction base="xsd:long">
			<xsd:pattern value="(((((0[1-9]|[12][0-9]|3[01])(0[13578]|10|12))|((0[1-9]|[12][0-9]|30)(0[469]|11))|((0[1-9]|1[0-9]|2[0-8])(02)))((1[89](\d{2}))|([2-9](\d{3}))))|((29)(02)((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))))(((((0[1-9]|[12][0-9]|3[01])(0[13578]|10|12))|((0[1-9]|[12][0-9]|30)(0[469]|11))|((0[1-9]|1[0-9]|2[0-8])(02)))((1[89](\d{2}))|([2-9](\d{3}))))|((29)(02)((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))))"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gültige Datumsangaben in der Form JJJJMMTTJJJJMMTT -->
	<xsd:simpleType name="DOPPEL_DATUM_NEU">
		<xsd:restriction base="xsd:long">
			<xsd:pattern value="(((((1[89](\d{2}))|([2-9](\d{3})))(((0[13578]|10|12)(0[1-9]|[12][0-9]|3[01]))|((0[469]|11)(0[1-9]|[12][0-9]|30))|((02)(0[1-9]|1[0-9]|2[0-8])))))|(((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))(02)(29)))(((((1[89](\d{2}))|([2-9](\d{3})))(((0[13578]|10|12)(0[1-9]|[12][0-9]|3[01]))|((0[469]|11)(0[1-9]|[12][0-9]|30))|((02)(0[1-9]|1[0-9]|2[0-8])))))|(((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))(02)(29)))"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gültige Datumsangabe in der Form TTMMJJJJ (TT=Tag, MM=Monat, JJJJ=Jahr) und
	   - zusätzlich erlaubter Wertebereich: 00MMJJJJ, 0000JJJJ, 00000000 -->
	<xsd:simpleType name="ERSATZ_DATUM">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="(((((0[1-9]|[12][0-9]|3[01])(0[13578]|10|12))|((0[1-9]|[12][0-9]|30)(0[469]|11))|((0[1-9]|1[0-9]|2[0-8])(02)))((1[89](\d{2}))|([2-9](\d{3}))))|((29)(02)((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))))|(00(0[1-9]|1[0-2])[1-2][0-9]{3})|(0000[1-2][0-9]{3})|00000000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gültige Datumsangabe in der Form JJJJMMTT (TT=Tag, MM=Monat, JJJJ=Jahr) und
	   - zusätzlich erlaubter Wertebereich: JJJJMM00, JJJJ0000 und 00000000 -->
	<xsd:simpleType name="ERSATZ_DATUM_NEU">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="(((((1[89](\d{2}))|([2-9](\d{3})))(((0[13578]|10|12)(0[1-9]|[12][0-9]|3[01]))|((0[469]|11)(0[1-9]|[12][0-9]|30))|((02)(0[1-9]|1[0-9]|2[0-8])))))|(((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))(02)(29)))|[1-2][09][0-9][0-9](([0][0-9])|10|11|12)00|[0]{8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gebührennummer -->
	<xsd:simpleType name="GNR">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{5}[A-Z]{0,1}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gebührenordnung (GO) -->
	<xsd:simpleType name="GO">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ICD-Codes in der Form: ann, ann.n, ann.nn, ann.n- -->
	<xsd:simpleType name="ICD_CODE">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="3"/>
			<xsd:maxLength value="6"/>
			<!--xsd:pattern value="[A-Z][0-9]{2}|[A-Z][0-9]{2}\.([0-9]{1,2}|[0-9]-)"/-->
			<!--xsd:pattern value="[A-Z][0-9]{2}|[A-Z][0-9]{2}\.([0-9]{1,2}|[0-9]-)|[A-Z]{3}"/-->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ICD-Codes in der Form ann, ann.n, ann.-, ann.n- und ann.nn -->
	<xsd:simpleType name="ICD_CODE_STAMM">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([A-Z][0-9]{2}|[A-Z][0-9]{2}.([0-9]{1,2}|[0-9]-|-))|UUU"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Institutionskennzeichen (IK) -->
	<xsd:simpleType name="IK">
		<xsd:restriction base="xsd:long">
			<xsd:pattern value="\d{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Ja/nein Angabe -->
	<xsd:simpleType name="JA_NEIN">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="J"/>
			<xsd:enumeration value="N"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Art des Fehlers beim Altersbezug -->
	<xsd:simpleType name="KANN_MUSS_FEHLER">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="k"/>
			<xsd:enumeration value="m"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Kostenträger-Abrechnungsbereich (KTAB) -->
	<xsd:simpleType name="KTAB">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="00"/>
			<xsd:enumeration value="01"/>
			<xsd:enumeration value="02"/>
			<xsd:enumeration value="03"/>
			<xsd:enumeration value="04"/>
			<xsd:enumeration value="05"/>
			<xsd:enumeration value="06"/>
			<xsd:enumeration value="07"/>
			<xsd:enumeration value="08"/>
			<xsd:enumeration value="09"/>
