<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- edited with XMLSpy v2008 rel. 2 sp1 (http://www.altova.com) by User (Kassenärztliche Bundesvereinigung) -->
<!-- Mit XMLSpy v2017 rel. 3 (http://www.altova.com) von Alexander Börner (Kassenärztliche Bundesvereinigung) bearbeitet -->
<!--

Projekt: arv (Arzneimittelvereinbarungen)
Namespace: "urn:ehd/arv", Präfix: arv
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:ehd/arv/001" xmlns:ehd="urn:ehd/001" targetNamespace="urn:ehd/arv/001" elementFormDefault="qualified">
	<xs:import namespace="urn:ehd/001" schemaLocation="datentypen_V1.40.xsd"/>
	<!-- ======================================================================= 
	Definitionen (arv_data)
	======================================================================= -->
	<!-- element ............................................................. -->
	<xs:element name="pzn" type="pzn_typ"/>
	<xs:element name="handelsname" type="medicine_form_type"/>
	<xs:element name="standardaggregat" type="medicine_form_type"/>
	<xs:element name="wirkstoff" type="medicine_form_type"/>
	<xs:element name="atc4" type="medicine_form_type"/>
	<xs:element name="atc5" type="medicine_form_type"/>
	<xs:element name="wirkstoffgruppe" type="medicine_form_type"/>
	<xs:element name="warnhinweis_pvs">
		<xs:complexType>
			<xs:attribute name="V" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ddd_je_packung">
		<xs:complexType>
			<xs:attribute name="V" type="xs:decimal" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="darreichungsform">
		<xs:complexType>
			<xs:attribute name="V" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[A-Z]{3}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="S" type="xs:string" use="required" fixed="1.2.276.0.76.3.1.1.5.2.15"/>
			<xs:attribute name="einschluss" type="xs:boolean" default="true"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="warnhinweis_ersetzung">
		<xs:complexType>
			<xs:attribute name="V" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="hinweis_verordnung_identisch">
		<xs:complexType>
			<xs:attribute name="V" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="verweis_dokument">
		<xs:complexType>
			<xs:attribute name="V" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="iww_anmerkung">
		<xs:annotation>
			<xs:documentation>Eine Anmerkung des IWW zu dem Wirkstoff.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="link" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Einer Liste von Alternativen beschrieben anhand eines ATC-Codes auf die im beschreibeneden Text (s.a. Attribut "V" des Elternelements) verwiesen werden.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="darreichungsform" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
						<xs:attribute name="atc" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>Eine Verordnungsalternative angegeben durch den zugeordneten ATC-Code.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="beschreibung" use="required">
							<xs:annotation>
								<xs:documentation>Der Text des ATC-Codes.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="iww_kategorie" type="iww_kategorie_typ" use="optional">
							<xs:annotation>
								<xs:documentation>Die IWW-Kategorie der verlinkten Alternative im Kontext der übegrordneten Indikation.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="V">
				<xs:annotation>
					<xs:documentation>Der Text der Anmerkung.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!-- simpleType..................................-->
	<xs:simpleType name="europreis">
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="10"/>
			<xs:fractionDigits value="2"/>
			<xs:minInclusive value="0.00"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="prozentwert">
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="5"/>
			<xs:fractionDigits value="2"/>
			<xs:minInclusive value="0.00"/>
			<xs:maxInclusive value="100.00"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zielkosten_euro">
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="10"/>
			<xs:fractionDigits value="4"/>
			<xs:minInclusive value="0.0000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="geschlecht_value">
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:enumeration value="0"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zeit_einheit_value">
		<xs:annotation>
			<xs:documentation> Ein Wert für die Zeiteinheit, T:Tag, W:Woche,M:Monat,J:Jahr</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="T">
				<xs:annotation>
					<xs:documentation>Tag</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="W">
				<xs:annotation>
					<xs:documentation>Woche</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="M">
				<xs:annotation>
					<xs:documentation>Monat</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="J">
				<xs:annotation>
					<xs:documentation>Jahr</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="pzn_string_typ">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="iww_indikations_schluessel_typ">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="iww_kategorie_typ">
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="4a"/>
			<xs:enumeration value="4b"/>
			<xs:enumeration value="4c"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="5a"/>
			<xs:enumeration value="5b"/>
			<xs:enumeration value="5c"/>
			<xs:enumeration value="6"/>
			<xs:enumeration value="6a"/>
			<xs:enumeration value="6b"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kommentar_typ">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="color_rgb_enum_typ">
		<xs:restriction base="xs:string">
			<xs:pattern value="#[0-9A-F]{6,6}|Aqua|Black|Blue|Fuchsia|Gray|Green|Lime|Maroon|Navy|Olive|Purple|Red|Silver|Teal|White|Yellow|MistyRose|Orange"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- complexType ........................................................... -->
	<xs:complexType name="geschlecht_typ">
		<xs:attribute name="V" type="geschlecht_value" use="required"/>
		<xs:attribute name="S" use="required" fixed="1.2.276.0.76.5.232"/>
	</xs:complexType>
	<xs:complexType name="nonNegativeInteger_typ">
		<xs:attribute name="V" type="xs:nonNegativeInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="zeit_einheit_typ">
		<xs:attribute name="V" type="zeit_einheit_value" use="required"/>
	</xs:complexType>
	<xs:complexType name="zeitraum_complex_typ">
		<xs:annotation>
			<xs:documentation>Beschreibt einen  Zeitraum.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="von" type="nonNegativeInteger_typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Anzahl der Zeiteinheiten</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bis" type="nonNegativeInteger_typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Anzahl der Zeiteinheiten</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unit" type="zeit_einheit_typ" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Zeiteinheit</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="pzn_basis_typ">
		<xs:sequence>
			<xs:element ref="ddd_je_packung"/>
		</xs:sequence>
		<xs:attribute name="V" type="pzn_string_typ" use="required"/>
	</xs:complexType>
	<xs:complexType name="pzn_typ">
		<xs:sequence>
			<xs:element ref="ddd_je_packung" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="V" type="pzn_string_typ" use="required"/>
		<xs:attribute name="einschluss" type="xs:boolean" default="true"/>
	</xs:complexType>
	<xs:complexType name="medicine_base_type">
		<xs:attribute name="V" type="xs:string" use="required"/>
		<xs:attribute name="einschluss" type="xs:boolean" default="true"/>
	</xs:complexType>
	<xs:complexType name="medicine_form_type">
		<xs:complexContent>
			<xs:extension base="medicine_base_type">
				<xs:sequence>
					<xs:element ref="darreichungsform" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="medicine_form_iww_type">
		<xs:complexContent>
			<xs:extension base="medicine_form_type">
				<xs:sequence>
					<xs:element ref="iww_anmerkung" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
				<xs:attribute name="bezeichnung">
					<xs:annotation>
						<xs:documentation>Die Bezeichnung des über das V-Attribut angegebenen Eintrags.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="iww_kategorie" type="iww_kategorie_typ" use="optional">
					<xs:annotation>
						<xs:documentation>Die IWW-Kategorie des Arzneimittels/Wirkstoffs.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="fachgruppen_base_type">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="fachgruppenintervall">
				<xs:complexType>
					<xs:attribute name="intervall_start" type="xs:positiveInteger" use="optional"/>
					<xs:attribute name="intervall_stop" type="xs:positiveInteger" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="fachgruppennummer">
				<xs:complexType>
					<xs:attribute name="V" type="xs:positiveInteger" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="fachgruppen_liste_base_type">
		<xs:choice minOccurs="1" maxOccurs="unbounded">
			<xs:element name="fachgruppenintervall">
				<xs:complexType>
					<xs:attribute name="intervall_start" type="xs:positiveInteger" use="optional"/>
					<xs:attribute name="intervall_stop" type="xs:positiveInteger" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="fachgruppennummer">
				<xs:complexType>
					<xs:attribute name="V" type="xs:positiveInteger" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="bedingung_typ">
		<xs:annotation>
			<xs:documentation>Eine Bedingung die erfüllt sein muss, damit die Regelung angewendet werden kann.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bedingung" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="altersgrenze" type="zeitraum_complex_typ" minOccurs="0"/>
						<xs:element name="geschlecht" type="geschlecht_typ" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute name="einschluss" type="xs:boolean" default="true"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="infomassnahmen_bedingung_typ">
		<xs:annotation>
			<xs:documentation>Eine Bedingung die erfüllt sein muss, damit die Regelung angewendet werden kann.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bedingung" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="altersgrenze" type="zeitraum_complex_typ" minOccurs="0"/>
						<xs:element name="geschlecht" type="geschlecht_typ" minOccurs="0"/>
						<xs:element name="fachgruppen_liste" type="fachgruppen_liste_base_type" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute name="einschluss" type="xs:boolean" default="true"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="iww_bedingung_typ">
		<xs:sequence>
			<xs:element name="bedingung" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="altersgrenze" type="zeitraum_complex_typ" minOccurs="0"/>
						<xs:element name="geschlecht" type="geschlecht_typ" minOccurs="0"/>
						<xs:element name="iww_indikationen" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="indikation" type="iww_indikations_schluessel_typ" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="fachgruppen_liste" type="fachgruppen_liste_base_type" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute name="einschluss" type="xs:boolean" default="true"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ======================================================================= 
   hinweis_intervall_typ zur Definition der Grenzen, für die ein Hinweis angezeigt werden soll.
   Wenn "inklusion_intervallgrenze" = true bei "intervall_start" gesetzt, so handelt es sich um ein größer gleich.
   Wenn "inklusion_intervallgrenze" = false bei "intervall_start" gesetzt, so handelt es sich um ein größer.
   Wenn "inklusion_intervallgrenze" = true bei "intervall_stop" gesetzt, so handelt es sich um ein kleiner gleich.
   Wenn "inklusion_intervallgrenze" = false bei "intervall_start" gesetzt, so handelt es sich um ein kleiner.
	======================================================================= -->
	<xs:complexType name="hinweis_intervall_typ">
		<xs:sequence>
			<xs:element name="intervall_start" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="V" type="xs:decimal" use="required"/>
					<xs:attribute name="inklusion_intervallgrenze" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="intervall_stop" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="V" type="xs:decimal" use="required"/>
					<xs:attribute name="inklusion_intervallgrenze" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="hinweis" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ======================================================================= 
   Definition der Typs Leitsubstanzquotenregelung
	======================================================================= -->
	<xs:complexType name="leitsubstanzquotenregelung_typ">
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="pzn"/>
				<xs:element ref="handelsname"/>
				<xs:element ref="standardaggregat"/>
				<xs:element ref="wirkstoff"/>
				<xs:element ref="atc5"/>
				<xs:element ref="wirkstoffgruppe"/>
				<xs:element ref="atc4"/>
			</xs:choice>
			<xs:element name="leitsubstanz" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:choice maxOccurs="unbounded">
							<xs:element ref="pzn"/>
							<xs:element ref="handelsname"/>
							<xs:element ref="standardaggregat"/>
							<xs:element ref="wirkstoff"/>
							<xs:element ref="atc5"/>
							<xs:element ref="wirkstoffgruppe"/>
							<xs:element ref="atc4"/>
						</xs:choice>
						<xs:element name="fachgruppenquote" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="fachgruppen_base_type">
										<xs:sequence>
											<xs:choice minOccurs="0">
												<xs:element name="quote_leitsubstanz_ddd">
													<xs:complexType>
														<xs:attribute name="V" type="xs:decimal" use="required"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="quote_leitsubstanz_verordnungen">
													<xs:complexType>
														<xs:attribute name="V" type="xs:decimal" use="required"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="quote_leitsubstanz_bruttoumsatz">
													<xs:complexType>
														<xs:attribute name="V" type="xs:decimal" use="required"/>
													</xs:complexType>
												</xs:element>
											</xs:choice>
											<xs:element ref="warnhinweis_ersetzung" minOccurs="0"/>
											<xs:element ref="hinweis_verordnung_identisch" minOccurs="0"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ======================================================================= 
   Definition des Typs Zielpreisregelung
	======================================================================= -->
	<xs:complexType name="zielpreisregelung_typ">
		<xs:sequence>
			<xs:element ref="pzn" maxOccurs="unbounded"/>
			<xs:element name="zielpreis">
				<xs:complexType>
					<xs:attribute name="V" type="europreis" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="kategorie_zielpreisregelung" type="hinweis_intervall_typ" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ======================================================================= 
   Definition des kostenvergleich_ddd_typ
	======================================================================= -->
	<xs:complexType name="kostenvergleich_ddd_typ">
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="pzn"/>
				<xs:element ref="handelsname"/>
				<xs:element ref="standardaggregat"/>
				<xs:element ref="wirkstoff"/>
				<xs:element ref="atc5"/>
				<xs:element ref="wirkstoffgruppe"/>
				<xs:element ref="atc4"/>
			</xs:choice>
			<xs:element name="zielkosten_je_ddd">
				<xs:complexType>
					<xs:attribute name="V" type="xs:decimal" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="fachgruppenquote" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="fachgruppen_base_type">
							<xs:sequence>
								<xs:element name="quote_kostenvergleich_ddd" minOccurs="0">
									<xs:complexType>
										<xs:attribute name="V" type="xs:decimal" use="required"/>
									</xs:complexType>
								</xs:element>
								<xs:element name="kategorie_ddd_kosten_vergleich" type="hinweis_intervall_typ" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ======================================================================= 
   Definition des generikaquotenregelung_typ
	======================================================================= -->
	<xs:complexType name="generikaquotenregelung_typ">
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="pzn_original" type="pzn_typ"/>
				<xs:element name="handelsname_original" type="medicine_form_type"/>
				<xs:element name="standardaggregat_original" type="medicine_form_type"/>
			</xs:choice>
			<xs:element name="generischer_wirkstoff_alternative" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="pzn"/>
						<xs:element ref="handelsname"/>
						<xs:element ref="standardaggregat"/>
						<xs:element ref="atc5"/>
						<xs:element ref="wirkstoff"/>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="fachgruppenquote" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="fachgruppen_base_type">
							<xs:sequence>
								<xs:choice minOccurs="0">
									<xs:element name="quote_generika_ddd" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="pzn_generika" type="pzn_basis_typ" maxOccurs="unbounded"/>
											</xs:sequence>
											<xs:attribute name="V" type="xs:decimal" use="required"/>
										</xs:complexType>
									</xs:element>
									<xs:element name="quote_generika_verordnungen" minOccurs="0">
										<xs:complexType>
											<xs:choice minOccurs="0" maxOccurs="unbounded">
												<xs:element ref="pzn"/>
												<xs:element ref="handelsname"/>
												<xs:element ref="standardaggregat"/>
												<xs:element ref="wirkstoff"/>
												<xs:element ref="atc5"/>
												<xs:element ref="wirkstoffgruppe"/>
