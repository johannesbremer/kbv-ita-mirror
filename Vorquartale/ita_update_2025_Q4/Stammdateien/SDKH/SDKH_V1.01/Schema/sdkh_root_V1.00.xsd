	======================================================================= -->
	<xs:element name="ehd" type="sdkh_root_typ">
		<xs:annotation>
			<xs:documentation>root element ehd "eHealthData"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<!-- ======================================================================= 
	das abgeleitete sdkh_root_typ des Basistyps: ehd_root_typ
