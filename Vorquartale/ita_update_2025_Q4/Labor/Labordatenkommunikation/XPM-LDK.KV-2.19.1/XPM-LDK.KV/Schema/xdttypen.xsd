	<!-- Format: alnum, Feldlänge: var -->
	<xsd:simpleType name="ALNUM_VAR">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="256"/>
			<xsd:pattern value=".*\S.*"/>
		</xsd:restriction>
	</xsd:simpleType>

