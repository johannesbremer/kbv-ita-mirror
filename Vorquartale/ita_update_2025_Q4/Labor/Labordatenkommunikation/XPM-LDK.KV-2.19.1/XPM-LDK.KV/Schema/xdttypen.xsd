	<!-- Formatregeln -->
	<!-- nnnnn -->
	<xsd:simpleType name="F001">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{5}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Format Datum: JJJJMMTT -->
	<xsd:simpleType name="F002">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="(((((1[89](\d{2}))|([2-9](\d{3})))(((0[13578]|10|12)(0[1-9]|[12][0-9]|3[01]))|((0[469]|11)(0[1-9]|[12][0-9]|30))|((02)(0[1-9]|1[0-9]|2[0-8])))))|(((1[89]0[48])|(1[89][2468][048])|(1[89][13579][26])|([2468][048]00)|([2-9][0-9]0[48])|([2-9][0-9][2468][048])|([2-9][0-9][13579][26])|([3579][26]00))(02)(29)))"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gültige Datumsangabe in der Form JJJJMMTT (TT=Tag, MM=Monat, JJJJ=Jahr) und
	   - zusätzlich erlaubter Wertebereich: JJJJMM00, JJJJ0000, 00000000 -->
	<xsd:simpleType name="F003">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[1-2][09][0-9][0-9][0-1][0-9][0-3][0-9]|[0]{8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ann, ann.n, ann.nn, ann.n- ann.- -->
	<xsd:simpleType name="F004">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([A-Z][0-9]{2})(\.((\d?\-)|(\d{1,2})))?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- WWT -->
	<xsd:simpleType name="F005">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-5][0-9][0-6]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- hhmm Wertebereich: hh = 00 - 99 mm = 00 - 59 -->
	<xsd:simpleType name="F006">
		<xsd:restriction base="xsd:unsignedShort">
			<xsd:pattern value="[0-9][0-9][0-5][0-9]"/>
			<xsd:maxInclusive value='9959'/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- n.n.n -->
	<xsd:simpleType name="F007">
		<!-- E001 & F007 -->
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="LDT((\d\.\d\.\d{1,4})|(\d\.\d\d\.\d{1,4}))"/>
			<!-- xsd:enumeration value="LDT3.2.5"/-->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- G-alpha[n[n[n[n]]][K-alpha[aerw]]][/Lkz] oder [G-alpha]n[n[n[n]]][K-alpha[aerw]][/Lkz] -->
	<xsd:simpleType name="F008">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value=".*\S.*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- nnnnn, nnnnn[G-alpha] -->
	<xsd:simpleType name="F009">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value=".*\S.*"/>
			<!--xsd:pattern value="\d{5}[A-Z]{0,1}"/-->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- kknnnnnmm -->
	<xsd:simpleType name="F010">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- nnnnnnmff -->
	<xsd:simpleType name="F011">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- a/nn/JJMM/MM/aaa -->
	<xsd:simpleType name="F012">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[VXYZ]/\d{2}/\d{2}(0[1-9]|1[0-2])/\d{2}/[a-zA-Z0-9]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- annnnnnnnP -->
	<xsd:simpleType name="F013">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z]\d{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- 00nnnnnnP -->
	<xsd:simpleType name="F014">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="00\d{7}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- nnn/nnn -->
	<xsd:simpleType name="F015">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{3}/\d{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- HHmmss(ms(000-999)) -->
	<xsd:simpleType name="F016">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="[0-2][0-9][0-5][0-9][0-5][0-9]{1,4}"/>
			<xsd:maxInclusive value='235959999'/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- JJJJ -->
	<xsd:simpleType name="F017">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="[1-2][0-9]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- JJJJMMTT, JJJJMM00, JJJJ0000, da das Jahr im Prinzip 0000 sein kann, hier keine weiteren Einschränkungen -->
	<xsd:simpleType name="F018">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:pattern value="[0-9]{8}"/>
			<xsd:minExclusive value="0"></xsd:minExclusive>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="F022">
			<xsd:restriction base="xsd:string">
			<xsd:pattern value="555555\d{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="F023">
			<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{1,2}(-\d{1,2})?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="F024">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\w+)|(([\w]+:)?([\w]+)([/_][\w]+)*)"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Gebührennummer -->
	<xsd:simpleType name="GNR">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{5}[A-Z]{0,1}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<!-- Leistungskennzeichen -->
	<xsd:simpleType name="LEISTUNG_KZ">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\d|[A-Z]|#|$|&lt;|&gt;|\*|k){1,3}"/>
		</xsd:restriction>
	</xsd:simpleType>

