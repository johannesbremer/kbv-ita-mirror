	<!-- Bei nicht numerischen Werten: N, A, AA -->
	<xsd:simpleType name="E005">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="H{1,2}|\+{1,2}|L{1,2}|\-{1,2}|N|A{1,2}|!H|!\+|!L|!-"/>
