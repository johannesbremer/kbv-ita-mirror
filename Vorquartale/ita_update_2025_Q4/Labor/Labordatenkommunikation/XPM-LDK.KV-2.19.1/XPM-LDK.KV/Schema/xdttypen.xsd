		</xsd:restriction>
	</xsd:simpleType>
	<!-- 01-99 -->
	<xsd:simpleType name="E003">
		<xsd:restriction base="xsd:unsignedShort">
			<xsd:pattern value="\d{3}"/>
			<xsd:minInclusive value='002'/>
			<xsd:maxInclusive value='999'/>
		</xsd:restriction>
	</xsd:simpleType>
