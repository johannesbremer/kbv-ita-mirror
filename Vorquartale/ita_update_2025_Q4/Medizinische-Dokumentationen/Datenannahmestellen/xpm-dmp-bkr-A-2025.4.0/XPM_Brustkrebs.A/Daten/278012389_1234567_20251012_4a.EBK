	<body>
		<section>
			<paragraph>
				<caption>
					<caption_cd DN="Einschreibung"/>
				</caption>
				<content>
					<local_markup descriptor="sciphox" ignore="all">
						<sciphox:sciphox-ssu country="de" version="v1" type="observation">
							<sciphox:Beobachtungen>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Lokoregionäres Rezidiv Datum der histologischen Sicherung"/>
									<sciphox:Zeitpunkt_dttm V="2023-02-02"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Fernmetastasen Datum der diagnostischen Sicherung von Fernmetastasen"/>
									<sciphox:Zeitpunkt_dttm V="2023-02-02"/>
								</sciphox:Beobachtung>
							</sciphox:Beobachtungen>
						</sciphox:sciphox-ssu>
					</local_markup>
				</content>
			</paragraph>
			<paragraph>
				<caption>
					<caption_cd DN="Aktueller Befundstatus des Primärtumors/kontralateralen Brustkrebses"/>
				</caption>
				<content>
					<local_markup descriptor="sciphox" ignore="all">
						<sciphox:sciphox-ssu country="de" version="v1" type="observation">
							<sciphox:Beobachtungen>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="T"/>
									<sciphox:Ergebnistext V="X"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="N"/>
									<sciphox:Ergebnistext V="3"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="M"/>
									<sciphox:Ergebnistext V="0"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Hormonrezeptorstatus Östrogen und/oder Progesteron (gemäß Immunreaktiver Score [IRS])"/>
									<sciphox:Ergebnistext V="Positiv"/>
								</sciphox:Beobachtung>
							</sciphox:Beobachtungen>
						</sciphox:sciphox-ssu>
					</local_markup>
				</content>
			</paragraph>
			<paragraph>
				<caption>
					<caption_cd DN="Behandlung des Primärtumors/kontralateralen Brustkrebses"/>
				</caption>
				<content>
					<local_markup descriptor="sciphox" ignore="all">
						<sciphox:sciphox-ssu country="de" version="v1" type="observation">
							<sciphox:Beobachtungen>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Aktuelle adjuvante endokrine Therapie"/>
									<sciphox:Ergebnistext V="Aromataseinhibitoren"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Nebenwirkungen der aktuellen adjuvanten endokrinen Therapie"/>
									<sciphox:Ergebnistext V="stark belastend"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Fortführung der adjuvanten endokrinen Therapie"/>
									<sciphox:Ergebnistext V="Regulär nach 5 Jahren abgeschlossen"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Bei Patientinnen unter adjuvanter Therapie mit Aromataseinhibitoren, sofern die Absicht für eine spezifische medikamentöse Therapie einer Osteoporose besteht: DXA-Befund"/>
									<sciphox:Ergebnistext V="Auffällig"/>
								</sciphox:Beobachtung>
							</sciphox:Beobachtungen>
						</sciphox:sciphox-ssu>
					</local_markup>
				</content>
			</paragraph>
			<paragraph>
				<caption>
					<caption_cd DN="Befunde und Therapie von Fernmetastasen"/>
				</caption>
				<content>
					<local_markup descriptor="sciphox" ignore="all">
						<sciphox:sciphox-ssu country="de" version="v1" type="observation">
							<sciphox:Beobachtungen>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Lokalisation von Fernmetastasen"/>
									<sciphox:Ergebnistext V="Knochen"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Therapie bei Knochenmetastasen (Bisphosphonate)"/>
									<sciphox:Ergebnistext V="Nein"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Therapie bei Knochenmetastasen (Denosumab)"/>
									<sciphox:Ergebnistext V="Nein"/>
									<sciphox:Ergebnistext V="Kontraindikation"/>
								</sciphox:Beobachtung>
							</sciphox:Beobachtungen>
						</sciphox:sciphox-ssu>
					</local_markup>
				</content>
			</paragraph>
			<paragraph>
				<caption>
					<caption_cd DN="Sonstige Befunde"/>
				</caption>
				<content>
					<local_markup descriptor="sciphox" ignore="all">
						<sciphox:sciphox-ssu country="de" version="v1" type="observation">
							<sciphox:Beobachtungen>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Symptomatisches Lymphödem"/>
									<sciphox:Ergebnistext V="Nein"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Empfehlung zu regelmäßigem körperlichen Training abgegeben"/>
									<sciphox:Ergebnistext V="Nein"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Z. n. besonders kardiotoxischer Tumortherapie"/>
									<sciphox:Ergebnistext V="Nein"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Körpergröße"/>
									<sciphox:Ergebniswert V="1.70" U="m"/>
								</sciphox:Beobachtung>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Körpergewicht"/>
									<sciphox:Ergebniswert V="085" U="kg"/>
								</sciphox:Beobachtung>
							</sciphox:Beobachtungen>
						</sciphox:sciphox-ssu>
					</local_markup>
				</content>
			</paragraph>
			<paragraph>
				<caption>
					<caption_cd DN="Behandlungsplanung"/>
				</caption>
				<content>
					<local_markup descriptor="sciphox" ignore="all">
						<sciphox:sciphox-ssu country="de" version="v1" type="observation">
							<sciphox:Beobachtungen>
								<sciphox:Beobachtung>
									<sciphox:Parameter DN="Dokumentationsintervall"/>
									<sciphox:Ergebnistext V="Halbjährlich oder häufiger"/>
								</sciphox:Beobachtung>
							</sciphox:Beobachtungen>
						</sciphox:sciphox-ssu>
					</local_markup>
				</content>
			</paragraph>
