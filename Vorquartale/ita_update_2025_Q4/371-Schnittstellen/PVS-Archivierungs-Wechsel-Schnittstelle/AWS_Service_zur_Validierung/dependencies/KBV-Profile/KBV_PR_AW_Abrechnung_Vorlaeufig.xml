        <key value="activeOrInactive" />
        <severity value="error" />
        <human value="Es ist nur aktiv (active) oder cancelled oder vorbereitet(draft) erlaubt." />
        <expression value="status='active' or status='cancelled' or status='draft'" />
        <source value="https://fhir.kbv.de/StructureDefinition/KBV_PR_AW_Abrechnung_Vorlaeufig" />
      </constraint>
